<?xml version="1.0" encoding="UTF-8" ?>
<configuration scan="true" scanPeriod="5 seconds">
    <appender name="out" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%5level]:%1marker%msg%n</pattern>
        </encoder>
    </appender>

    <logger name="org.jungletree.clientconnector" level="INFO"/>

    <root level="WARN">
        <appender-ref ref="out" />
    </root>
</configuration>
